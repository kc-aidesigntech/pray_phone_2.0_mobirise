          "_name": "features04",
          "_sourceTheme": "flexm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"features04 flexm5\" group=\"Features\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <!-- Size Parameters -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n\n        <!-- Card Parameters -->\n        <header>Card</header>\n        <input type=\"color\" title=\"Content BG\" name=\"card\" value=\"#272b2e\"> <!-- Renamed for clarity: This is the content area background -->\n        <!-- Added parameter to control default number of feature rows -->\n        <input type=\"range\" inline title=\"Feature Rows\" name=\"featureRows\" min=\"1\" max=\"50\" step=\"1\" value=\"10\">\n        <!-- Removed Card Title/Desc checkboxes as they aren't directly used in the table display -->\n        <!-- <input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\" checked> -->\n        <!-- <input type=\"checkbox\" title=\"Card Description\" name=\"showDesc\" checked> -->\n\n        <!-- Background Parameters -->\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#15171a\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"../_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#F2F2F2\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <!-- Background Elements -->\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <!-- Main Content Container -->\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"items-wrapper\">\n                    <!-- Single Feature Item Structure -->\n                    <div class=\"item features-image\">\n                        <div class=\"item-wrapper\">\n                            <!-- Left Side: Content Area with Table -->\n                            <div class=\"item-content\" mbr-style=\"{'background-color': card}\">\n                                <div class=\"title-wrap\">\n                                    <!-- Table Definition -->\n                                    <table class=\"table\" cellspacing=\"0\">\n                                        <thead>\n                                            <!-- Single Header Row (No mbr-list needed if only one static header) -->\n                                            <tr class=\"table-heads\">\n                                                <!-- Single Header Column -->\n                                                <th mbr-text class=\"head-item1 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".head-item1\">\n                                                    Options</th>\n                                            </tr>\n                                        </thead>\n                                        <!-- Table Body: Managed by Mobirise -->\n                                        <!-- mbr-list tells Mobirise this is a list -->\n                                        <!-- mbr-list-grow=\"featureRows\" links it to the 'Feature Rows' parameter -->\n                                        <tbody mbr-list mbr-list-grow=\"featureRows\">\n                                            <!-- ****** SINGLE TEMPLATE ROW ****** -->\n                                            <!-- Mobirise uses this <tr> as a template to generate rows based on the 'featureRows' parameter value -->\n                                            <!-- DO NOT add mbr-list attributes to this <tr> -->\n                                            <tr>\n                                                <!-- Single Data Cell Template -->\n                                                <!-- mbr-text allows editing the text in Mobirise -->\n                                                <!-- data-app-selector links it to the CSS/JS if needed -->\n                                                <td mbr-text class=\"body-item1 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item1\">\n                                                    1 - Anxiety<!-- Default text for new rows -->\n                                                </td>\n                                            </tr>\n                                            <!-- ****** NO MORE STATIC <tr> elements here ****** -->\n                                        <tr>\n                                                <!-- Single Data Cell Template -->\n                                                <!-- mbr-text allows editing the text in Mobirise -->\n                                                <!-- data-app-selector links it to the CSS/JS if needed -->\n                                                <td mbr-text class=\"body-item1 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item1\">\n                                                    2 - Depression<!-- Default text for new rows -->\n                                                </td>\n                                            </tr><tr>\n                                                <!-- Single Data Cell Template -->\n                                                <!-- mbr-text allows editing the text in Mobirise -->\n                                                <!-- data-app-selector links it to the CSS/JS if needed -->\n                                                <td mbr-text class=\"body-item1 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item1\">3 - Bipolar<!-- Default text for new rows -->\n                                                </td>\n                                            </tr><tr>\n                                                <!-- Single Data Cell Template -->\n                                                <!-- mbr-text allows editing the text in Mobirise -->\n                                                <!-- data-app-selector links it to the CSS/JS if needed -->\n                                                <td mbr-text class=\"body-item1 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".body-item1\">4 - Trauma<!-- Default text for new rows --></td>\n                                            </tr><tr>\n                                                <!-- Single Data Cell Template -->\n                                                <!-- mbr-text allows editing the text in Mobirise -->\n                                                <!-- data-app-selector links it to the CSS/JS if needed -->\n                                                <td mbr-text class=\"body-item1 mbr-fonts-style\" mbr