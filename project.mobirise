          "_name": "slider01",
          "_sourceTheme": "flexm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"slider01 mbr-embla flexm5\" group=\"Sliders & Galleries\" plugins=\"embla\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Carousel</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"1\" max=\"10\" value=\"5\" step=\"1\" condition=\"autoplay\">\n        <input type=\"checkbox\" title=\"Loop\" name=\"loop\" checked>\n        <input type=\"checkbox\" title=\"Draggable\" name=\"draggable\" checked>\n        <input type=\"checkbox\" name=\"arrows\" title=\"Arrows\" checked>\n        <input type=\"checkbox\" name=\"arrowsMobile\" title=\"Mobile Arrows\" condition=\"arrows\" checked>\n        <input type=\"color\" title=\"Arrows Color\" value=\"#ffffff\" name=\"arrowsColor\" condition=\"arrows\" selected>\n        <input type=\"color\" title=\"Arrows BG\" value=\"#f96e47\" name=\"arrowsBg\" condition=\"arrows\" selected>\n        <input type=\"color\" title=\"Slider BG\" value=\"#15171a\" name=\"sliderBg\">\n        <header>Cards</header>\n        <input type=\"range\" name=\"gap\" title=\"Gap\" min=\"0\" max=\"24\" step=\"4\" value=\"4\">\n        <input type=\"checkbox\" title=\"Card Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Card Description\" name=\"showDesc\" checked>\n        <input type=\"checkbox\" title=\"Card Icon\" name=\"showIcon\" checked>\n        <input type=\"color\" title=\"Icon Color\" value=\"#b9b9b9\" name=\"icon\" condition=\"showIcon\">\n        <input type=\"checkbox\" title=\"Card Tags\" name=\"showTags\" checked>\n        <input type=\"range\" title=\"Tags Amount\" name=\"tags\" min=\"1\" max=\"3\" step=\"1\" value=\"3\" condition=\"showTags\">\n        <input type=\"color\" title=\"Tags BG 1\" value=\"#a672ff\" name=\"tag_1\" condition=\"showTags\">\n        <input type=\"color\" title=\"Tags BG 2\" value=\"#7dabab\" name=\"tag_2\" condition=\"showTags\">\n        <input type=\"color\" title=\"Tags BG 3\" value=\"#f96e47\" name=\"tag_3\" condition=\"showTags\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#b9b9b9\" name=\"border\">\n        <input type=\"color\" title=\"Card BG\" value=\"#272b2e\" name=\"card\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax required>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" selected parallax>\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#272b2e\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row content-wrapper\">\n            <div class=\"col-12 col-lg-10\">\n                <div class=\"embla\" data-loop=\"{{loop}}\" data-auto-play=\"{{autoplay}}\" data-auto-play-interval=\"{{interval}}\" data-draggable=\"{{draggable}}\" data-skip-snaps=\"true\" data-align=\"center\" data-contain-scroll=\"trimSnaps\">\n                    <div class=\"embla__viewport\">\n                        <div class=\"embla__container\">\n                            <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'px', 'margin-right': gap + 'px'}\">\n                                <div class=\"slide-content\">\n                                    <div class=\"title-wrap\">\n                                        <h4 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title, .icon-wrapper\" mbr-if=\"showTitle\">\n                                            Minimalist\n                                        </h4>\n                                    </div>\n                                    <div class=\"item-wrapper\">\n                                        <div class=\"item-wrap\">\n                                            <div class=\"item-img\">\n                                                <img src=\"@PROJECT_PATH@/assets/images/image24.jpg\" alt=\"Mobirise Website Builder\">\n                                            </div>\n                                            <div class=\"item-content\">\n                                                <div class=\"desc-wrap\" mbr-if=\"showDesc || showIcon\">\n                                                    <p class=\"item-desc mbr-fonts-style\" mbr-if=\"showDesc\" mbr-theme-style=\"display-4\" data-app-selector=\".item-desc\">\n                                                        Focus on clean\n                                                    </p>\n                                                    <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                                                        <span mbr-icon class=\"mbr-iconfont mobi-mbri-search mobi-mbri\"></span>\n                                                    </div>\n                        